    "# Get url from DVC\n",
    "import pandas as pd\n",
    "import dvc.api\n",
    "\n",
    "path='data/wine_original.csv'\n",
    "repo='./'\n",
    "commit_hash = 'v1'\n",
    "\n",
    "data_url = dvc.api.get_url(\n",
    "  path=path,\n",
    "  repo=repo,\n",
    "  rev = commit_hash\n",
    "  )\n",
    "\n",
    "data = pd.read_csv(data_url, sep=\";\")\n",
    "print(data)\n"
